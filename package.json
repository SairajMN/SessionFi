{
  "name": "sessionfi-protocol",
  "version": "0.2.0",
  "description": "SessionFi - Gasless DeFi Sessions with Intent-Based Final Settlement",
  "type": "module",
  "scripts": {
    "demo": "tsx src/demo/protocol-demo.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy:contracts": "cd contracts && forge script script/Deploy.s.sol --rpc-url $RPC_URL --broadcast",
    "deploy:hook": "cd contracts && forge script script/DeploySessionFiHook.s.sol --rpc-url https://eth-sepolia.g.alchemy.com/v2/Qi9YQb2VqoyJJDPq23fDedrn9v96qf70 --broadcast",
    "build:contracts": "cd contracts && forge build",
    "test:contracts": "cd contracts && forge test",
    "test:yellow": "tsx src/yellow/test-contract.ts",
    "test:lifi": "tsx src/lifi/test-lifi.ts"
  },
  "keywords": [
    "defi",
    "session",
    "state-channel",
    "sui",
    "yellow-network",
    "intent-based",
    "gasless",
    "uniswap-v4",
    "lifi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@lifi/sdk": "^3.15.5",
    "@lifi/types": "^17.59.0",
    "@mysten/bcs": "^2.0.1",
    "@mysten/sui": "^2.2.0",
    "@wagmi/core": "^2.14.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.11.0",
    "forge": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SairajMN/SessionFi.git"
  },
  "bugs": {
    "url": "https://github.com/SairajMN/SessionFi/issues"
