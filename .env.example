# ============================================================================
# SessionFi Environment Configuration
# ============================================================================
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to version control!

# ============================================================================
# PRIVATE KEYS (REQUIRED)
# ============================================================================
# Your wallet private key (hex string starting with 0x)
# WARNING: Never share or commit your private key!
PRIVATE_KEY=0x...your_private_key_here

# Optional: Separate key for engine operations
ENGINE_PRIVATE_KEY=0x...engine_private_key_here

# ============================================================================
# ETHEREUM / EVM NETWORKS
# ============================================================================

# Ethereum Mainnet
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# Or use public RPC: https://eth.llamarpc.com

# Sepolia Testnet (recommended for testing)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# Or use public RPC: https://rpc.sepolia.org

# Arbitrum One
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# Or use public RPC: https://arb1.arbitrum.io/rpc

# Optimism
OPTIMISM_RPC_URL=https://opt-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# Or use public RPC: https://mainnet.optimism.io

# Base
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# Or use public RPC: https://mainnet.base.org

# Polygon
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# ============================================================================
# YELLOW NETWORK
# ============================================================================

# Yellow Network WebSocket URL
YELLOW_NODE_URL=wss://testnet.yellow.org/ws

# Yellow Custodian Contract Address (DEPLOYED to Sepolia)
YELLOW_CUSTODIAN_ADDRESS=0x187EDBb934591DF0f078076214e0564DB1c883A4

# ============================================================================
# SUI NETWORK
# ============================================================================

# Sui Private Key (ed25519 format)
SUI_PRIVATE_KEY=your_sui_private_key_hex

# Sui Package ID (after deploying Move contracts)
SUI_PACKAGE_ID=0x...your_package_id

# Sui Network (mainnet, testnet, devnet)
SUI_NETWORK=testnet

# ============================================================================
# LI.FI INTEGRATION
# ============================================================================

# LI.FI Integrator ID (register at https://li.fi/)
LIFI_INTEGRATOR=sessionfi-amm

# LI.FI API URL (production or staging)
LIFI_API_URL=https://li.quest/v1
# For testing: https://staging.li.quest/v1

# ============================================================================
# UNISWAP V4
# ============================================================================

# Deployed SessionFi Hook Address (DEPLOYED to Sepolia)
SESSIONFI_HOOK_ADDRESS=0x73c44610f97f2560cD27c53370987B827DB30beA

# Uniswap v4 Pool Manager Address (per network)
UNISWAP_POOL_MANAGER_ADDRESS=0x...pool_manager_address

# ============================================================================
# API KEYS (OPTIONAL)
# ============================================================================

# Alchemy API Key
ALCHEMY_API_KEY=your_alchemy_api_key

# Infura Project ID
INFURA_PROJECT_ID=your_infura_project_id

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key

# ============================================================================
# DEVELOPMENT
# ============================================================================

# Enable debug logging
DEBUG=false

# Default chain ID for development
DEFAULT_CHAIN_ID=11155111

# Port for local development server
PORT=5173
